    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/dul-es#isSubordinatedTo -->

    <owl:ObjectProperty rdf:about="https://w3id.org/def/dul-es#isSubordinatedTo">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/def/dul-es#directlyFollows"/>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/def/dul-es#isRelatedToConcept"/>
        <owl:inverseOf rdf:resource="https://w3id.org/def/dul-es#isSuperordinatedTo"/>
        <rdfs:domain rdf:resource="https://w3id.org/def/dul-es#Concept"/>
        <rdfs:range rdf:resource="https://w3id.org/def/dul-es#Concept"/>
        <rdfs:comment xml:lang="en">Direct succession applied to concepts. E.g. the role &apos;Officer&apos; is subordinated to &apos;Director&apos;.</rdfs:comment>
        <rdfs:comment xml:lang="es">Sucesión directa aplicada a conceptos. Por ejemplo, el papel Oficial está subordinado al de Inspector.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/def/dul-es"/>
        <rdfs:label xml:lang="es">está subordinado a</rdfs:label>
        <rdfs:label xml:lang="en">is subordinated to</rdfs:label>
        <rdfs:label xml:lang="it">è subordinato a</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/dul-es#isSuperordinatedTo -->

    <owl:ObjectProperty rdf:about="https://w3id.org/def/dul-es#isSuperordinatedTo">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/def/dul-es#directlyPrecedes"/>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/def/dul-es#isRelatedToConcept"/>
        <rdfs:domain rdf:resource="https://w3id.org/def/dul-es#Concept"/>
        <rdfs:range rdf:resource="https://w3id.org/def/dul-es#Concept"/>
        <rdfs:comment xml:lang="en">Direct precedence applied to concepts. E.g. the role &apos;Executive&apos; is superordinated to &apos;DepartmentManager&apos;.</rdfs:comment>
        <rdfs:comment xml:lang="es">La relación inversa a &apos;está subordinado a&apos;.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/def/dul-es"/>
        <rdfs:label xml:lang="es">está superordinado a</rdfs:label>
        <rdfs:label xml:lang="en">is superordinated to</rdfs:label>
        <rdfs:label xml:lang="it">è superordinato a</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/dul-es#isTaskDefinedIn -->

    <owl:ObjectProperty rdf:about="https://w3id.org/def/dul-es#isTaskDefinedIn">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/def/dul-es#isDefinedIn"/>
        <rdfs:domain rdf:resource="https://w3id.org/def/dul-es#Task"/>
        <rdfs:range rdf:resource="https://w3id.org/def/dul-es#Description"/>
        <rdfs:comment xml:lang="en">A relation between a description and a task, e.g. the task &apos;boil&apos; is defined in a recipe for a cake.</rdfs:comment>
        <rdfs:comment xml:lang="es">Relación inversa de &apos;define tarea&apos;.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/def/dul-es"/>
        <rdfs:label xml:lang="es">es tarea definida en</rdfs:label>
        <rdfs:label xml:lang="en">is task defined in</rdfs:label>
        <rdfs:label xml:lang="it">è un task definito in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/dul-es#isTaskOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/def/dul-es#isTaskOf">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/def/dul-es#isRelatedToConcept"/>
        <rdfs:domain rdf:resource="https://w3id.org/def/dul-es#Task"/>
        <rdfs:range rdf:resource="https://w3id.org/def/dul-es#Role"/>
        <rdfs:comment xml:lang="en">A relation between roles and tasks, e.g. &apos;students have the duty of giving exams&apos; (i.e. the Role &apos;student&apos; hasTask the Task &apos;giving exams&apos;).</rdfs:comment>
        <rdfs:comment xml:lang="es">Relación inversa de &apos;tiene como tarea&apos;.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/def/dul-es"/>
        <rdfs:label xml:lang="es">es tarea de</rdfs:label>
        <rdfs:label xml:lang="en">is task of</rdfs:label>
        <rdfs:label xml:lang="it">è un obiettivo per</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/dul-es#isTimeIncludedIn -->

    <owl:ObjectProperty rdf:about="https://w3id.org/def/dul-es#isTimeIncludedIn">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/def/dul-es#hasSetting"/>
        <rdfs:domain rdf:resource="https://w3id.org/def/dul-es#TimeInterval"/>
        <rdfs:range rdf:resource="https://w3id.org/def/dul-es#Situation"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/def/dul-es"/>
        <rdfs:label xml:lang="es">es tiempo incluido en</rdfs:label>
        <rdfs:label xml:lang="en">is time included in</rdfs:label>
        <rdfs:label xml:lang="it">è un tempo nel contesto di</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/dul-es#isTimeIntervalOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/def/dul-es#isTimeIntervalOf">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/def/dul-es#isRegionFor"/>
        <rdfs:domain rdf:resource="https://w3id.org/def/dul-es#TimeInterval"/>
        <rdfs:range rdf:resource="https://w3id.org/def/dul-es#Event"/>
        <rdfs:comment xml:lang="es">Relación inversa de &apos;tiene intervalo de tiempo&apos;.</rdfs:comment>
        <rdfs:comment xml:lang="en">The generic relation between time intervals and events.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/def/dul-es"/>
        <rdfs:label xml:lang="es">es intervalo de tiempo de</rdfs:label>
        <rdfs:label xml:lang="it">intervallo temporale di</rdfs:label>
        <rdfs:label xml:lang="en">is time interval of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/dul-es#isTimeOfObservationOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/def/dul-es#isTimeOfObservationOf">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/def/dul-es#isRegionFor"/>
        <rdfs:domain rdf:resource="https://w3id.org/def/dul-es#TimeInterval"/>
        <rdfs:range rdf:resource="https://w3id.org/def/dul-es#Entity"/>
        <rdfs:comment xml:lang="en">A relation to represent a (past, present or future) TimeInterval at which an Entity is observable.
In order to encode a specific time, a data value should be related to the TimeInterval. 
An alternative way of representing time is the datatype property: hasIntervalDate</rdfs:comment>
        <rdfs:comment xml:lang="es">Relación inversa de &apos;es observable a lo largo de&apos;.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/def/dul-es"/>
        <rdfs:label xml:lang="es">es tiempo de observación de</rdfs:label>
        <rdfs:label xml:lang="en">is time of observation of</rdfs:label>
        <rdfs:label xml:lang="it">è il tempo di osservazione di</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/dul-es#isUnifiedBy -->

    <owl:ObjectProperty rdf:about="https://w3id.org/def/dul-es#isUnifiedBy">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/def/dul-es#associatedWith"/>
        <owl:inverseOf rdf:resource="https://w3id.org/def/dul-es#unifies"/>
        <rdfs:domain rdf:resource="https://w3id.org/def/dul-es#Collection"/>
        <rdfs:range rdf:resource="https://w3id.org/def/dul-es#Description"/>
        <rdfs:comment xml:lang="en">A Collection has a unification criterion, provided by a Description; for example, a community of practice can be unified by a shared theory or interest, e.g. the community that makes research on mirror neurons shares some core knowledge about mirror neurons, which can be represented as a Description MirrorNeuronTheory that unifies the community. There can be several unifying descriptions.</rdfs:comment>
        <rdfs:comment xml:lang="es">Relación inversa de &apos;unifica&apos;.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/def/dul-es"/>
        <rdfs:label xml:lang="es">está unificado por</rdfs:label>
        <rdfs:label xml:lang="en">is unified by</rdfs:label>
        <rdfs:label xml:lang="it">è unificato da</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/dul-es#nearTo -->

    <owl:ObjectProperty rdf:about="https://w3id.org/def/dul-es#nearTo">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/def/dul-es#associatedWith"/>
        <owl:inverseOf rdf:resource="https://w3id.org/def/dul-es#nearTo"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/def/dul-es#Entity"/>
        <rdfs:range rdf:resource="https://w3id.org/def/dul-es#Entity"/>
        <rdfs:comment xml:lang="en">Generic distance relation between any Entity(s). E.g. Rome is near to Florence, astronomy is near to physics.</rdfs:comment>
        <rdfs:comment xml:lang="es">Relación de distancia genérica entre entidades cualesquiera. Por ejemplo, Madrid está cerca de Toledo, la astronomía está cerca de la física.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/def/dul-es"/>
        <rdfs:label xml:lang="es">está cerca de</rdfs:label>
        <rdfs:label xml:lang="en">near to</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/dul-es#overlaps -->

    <owl:ObjectProperty rdf:about="https://w3id.org/def/dul-es#overlaps">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/def/dul-es#associatedWith"/>
        <owl:inverseOf rdf:resource="https://w3id.org/def/dul-es#overlaps"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/def/dul-es#Entity"/>
        <rdfs:range rdf:resource="https://w3id.org/def/dul-es#Entity"/>
        <rdfs:comment xml:lang="en">A schematic relation between any entities, e.g. &apos;the chest region overlaps with the abdomen region&apos;, &apos;my spoken words overlap with hers&apos;, &apos;the time of my leave overlaps with the time of your arrival&apos;, &apos;fibromyalgia overlaps with other conditions&apos;.
Subproperties and restrictions can be used to specialize overlaps for objects, events, time intervals, etc.</rdfs:comment>
        <rdfs:comment xml:lang="es">Una relación esquemática entre entidades cualesquiera, por ejemplo, “la región del pecho se solapa con la región del abdomen”, “las palabras que yo hablo se solapan con las tuyas”, “el tiempo de mi partida se solapa con el tiempo tu llegada”, “la fibromialgia se solapa con otras condiciones”. Se pueden utilizar subpropiedades y restricciones para especializar el solape de objetos, eventos, intervalos de tiempo, etc.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/def/dul-es"/>
        <rdfs:label xml:lang="en">overlaps</rdfs:label>
        <rdfs:label xml:lang="es">se solapa con</rdfs:label>
        <rdfs:label xml:lang="it">sovrapposto a</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/dul-es#parametrizes -->

    <owl:ObjectProperty rdf:about="https://w3id.org/def/dul-es#parametrizes">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/def/dul-es#classifies"/>
        <rdfs:domain rdf:resource="https://w3id.org/def/dul-es#Parameter"/>
        <rdfs:range rdf:resource="https://w3id.org/def/dul-es#Region"/>
        <rdfs:comment xml:lang="es">La relación entre un ‘parámetro’, por ejemplo, Límite de Edad, y una ‘región’, por ejemplo, “18 años”. Para una relación más orientada a los datos, véase ‘tiene valor’.</rdfs:comment>
        <rdfs:comment xml:lang="en">The relation between a Parameter, e.g. &apos;MajorAgeLimit&apos;, and a Region, e.g. &apos;18_year&apos;.
For a more data-oriented relation, see hasDataValue</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/def/dul-es"/>
        <rdfs:label xml:lang="es">parametriza</rdfs:label>
        <rdfs:label xml:lang="en">parametrizes</rdfs:label>
        <rdfs:label xml:lang="it">parametrizza</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/dul-es#precedes -->

    <owl:ObjectProperty rdf:about="https://w3id.org/def/dul-es#precedes">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/def/dul-es#associatedWith"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/def/dul-es#Entity"/>
        <rdfs:range rdf:resource="https://w3id.org/def/dul-es#Entity"/>
        <rdfs:comment xml:lang="en">A relation between entities, expressing a &apos;sequence&apos; schema. 
E.g. &apos;year 1999 precedes 2000&apos;, &apos;deciding what coffee to use&apos; precedes &apos;preparing coffee&apos;, &apos;World War II follows World War I&apos;, &apos;in the Milan to Rome autoroute, Bologna precedes Florence&apos;, etc.
It can then be used between tasks, processes, time intervals, spatially locate objects, situations, etc. 
Subproperties can be defined in order to distinguish the different uses.</rdfs:comment>
        <rdfs:comment xml:lang="es">Relación inversa de &apos;sigue&apos;.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/def/dul-es"/>
        <rdfs:label xml:lang="es">precede</rdfs:label>
        <rdfs:label xml:lang="it">precede</rdfs:label>
        <rdfs:label xml:lang="en">precedes</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/dul-es#realizes -->

    <owl:ObjectProperty rdf:about="https://w3id.org/def/dul-es#realizes">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/def/dul-es#associatedWith"/>
        <rdfs:domain rdf:resource="https://w3id.org/def/dul-es#InformationRealization"/>
        <rdfs:range rdf:resource="https://w3id.org/def/dul-es#InformationObject"/>
        <rdfs:comment xml:lang="en">A relation between an information realization and an information object, e.g. the paper copy of the Italian Constitution realizes the text of the Constitution.</rdfs:comment>
        <rdfs:comment xml:lang="es">Una relación entre una realización de información y un objeto de información, por ejemplo, la copia en papel de la Constitución Española realiza el texto de la Constitución.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/def/dul-es"/>
        <rdfs:label xml:lang="es">realiza</rdfs:label>
        <rdfs:label xml:lang="en">realizes</rdfs:label>
        <rdfs:label xml:lang="it">realizza</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/dul-es#realizesInformationAbout -->

    <owl:ObjectProperty rdf:about="https://w3id.org/def/dul-es#realizesInformationAbout">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/def/dul-es#associatedWith"/>
        <rdfs:domain rdf:resource="https://w3id.org/def/dul-es#InformationRealization"/>
        <rdfs:range rdf:resource="https://w3id.org/def/dul-es#Entity"/>
        <rdfs:comment xml:lang="es">La relación entre entidades y realizaciones de información, por ejemplo, entre España y una copia del texto de la Constitución Española.</rdfs:comment>
        <rdfs:comment xml:lang="en">The relation between entities and information realizations, e.g. between Italy and a paper copy of the text of the Italian Constitution.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/def/dul-es"/>
        <rdfs:label xml:lang="es">realiza información sobre</rdfs:label>
        <rdfs:label xml:lang="en">realizes information about</rdfs:label>
        <rdfs:label xml:lang="it">realizza informazione che si riferisce a a</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/dul-es#sameSettingAs -->

    <owl:ObjectProperty rdf:about="https://w3id.org/def/dul-es#sameSettingAs">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/def/dul-es#associatedWith"/>
        <owl:inverseOf rdf:resource="https://w3id.org/def/dul-es#sameSettingAs"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/def/dul-es#Entity"/>
        <rdfs:range rdf:resource="https://w3id.org/def/dul-es#Entity"/>
        <rdfs:comment xml:lang="en">A relation between two entities participating in a same Situation; e.g., &apos;Our company provides an antivenom service&apos; (the situation is the service, the two entities are the company and the antivenom).</rdfs:comment>
        <rdfs:comment xml:lang="es">Una relación entre dos entidades participantes en la misma ‘situación’, por ejemplo, “nuestra empresa proporciona un servicio antisuero” (la situación es el servicio, las dos entidades son la compañía y el antisuero).</rdfs:comment>
        <rdfs:isDefinedBy>https://w3id.org/def/dul-es</rdfs:isDefinedBy>
        <rdfs:label xml:lang="es">está en la misma situación que</rdfs:label>
        <rdfs:label xml:lang="en">is in the same setting as</rdfs:label>
        <rdfs:label xml:lang="it">è nella stessa situazione di</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/dul-es#satisfies -->

    <owl:ObjectProperty rdf:about="https://w3id.org/def/dul-es#satisfies">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/def/dul-es#associatedWith"/>
        <rdfs:domain rdf:resource="https://w3id.org/def/dul-es#Situation"/>
        <rdfs:range rdf:resource="https://w3id.org/def/dul-es#Description"/>
        <rdfs:comment xml:lang="en">A relation between a Situation and a Description, e.g. the execution of a Plan satisfies that plan.</rdfs:comment>
        <rdfs:comment xml:lang="es">Una relación entre una ‘situación’ y una ‘descripción’, por ejemplo la ejecución de un ‘plan’ satisface ese plan.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/def/dul-es"/>
        <rdfs:label xml:lang="es">satisface</rdfs:label>
        <rdfs:label xml:lang="en">satisfies</rdfs:label>
        <rdfs:label xml:lang="it">soddisfa</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/dul-es#specializes -->

    <owl:ObjectProperty rdf:about="https://w3id.org/def/dul-es#specializes">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/def/dul-es#associatedWith"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/def/dul-es#SocialObject"/>
        <rdfs:range rdf:resource="https://w3id.org/def/dul-es#SocialObject"/>
        <rdfs:comment xml:lang="en">A partial order relation that holds between social objects. 
It mainly represents the subsumption relation between e.g. a Concept or Description and another Concept (resp. Description) that is broader in extensional interpretation, but narrower in intensional interpretation. For example, the role PhDStudent specializes the role Student.
Another possible use is between a Collection that isCoveredBy a Concept A, and another Collection that isCoveredBy a Concept B that on its turm specializes A. For example, the 70,000 series Selmer Mark VI saxophone Collection specializes the Selmer Mark VI saxophone Collection.</rdfs:comment>
        <rdfs:comment xml:lang="es">Una relación de orden parcial que tiene lugar entre objetos sociales. Representa principalmente la relación de subsunción entre, por ejemplo, un ‘concepto’ o ‘descripción’ y otro concepto (‘descripción’ respectivamente) que es más amplia en la interpretación extensional, pero más estrecha en la interpretación intensional. Por ejemplo, el papel Estudiante de Doctorado especializa el papel Estudiante. Otro posible uso es entre una ‘colección’ que ‘es cubierta por’ un ‘concepto’ A, y otra ‘colección’ que es cubierta por un concepto B que, a su vez, especializa A. Por ejemplo, la serie de saxofones altos de la colección Selmer Mark VI especializa la colección Selmer Mark VI.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/def/dul-es"/>
        <rdfs:label xml:lang="es">especializa</rdfs:label>
        <rdfs:label xml:lang="en">specializes</rdfs:label>
        <rdfs:label xml:lang="it">specializza</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/dul-es#unifies -->

    <owl:ObjectProperty rdf:about="https://w3id.org/def/dul-es#unifies">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/def/dul-es#associatedWith"/>
        <rdfs:domain rdf:resource="https://w3id.org/def/dul-es#Description"/>
        <rdfs:range rdf:resource="https://w3id.org/def/dul-es#Collection"/>
        <rdfs:comment xml:lang="en">A Collection has a unification criterion, provided by a Description; for example, a community of practice can be unified by a shared theory or interest, e.g. the community that makes research on mirror neurons shares some core knowledge about mirror neurons, which can be represented as a Description MirrorNeuronTheory that unifies the community. There can be several unifying descriptions.</rdfs:comment>
        <rdfs:comment xml:lang="es">Una ‘colección’ tiene un criterio de unificación, proporcionado por una ‘descripción’; por ejemplo, una comunidad de práctica puede estar unificada por una teoría compartida o interés, por ejemplo, la comunidad que hace investigación en neuronas especulares comparte algunos conocimientos básicos sobre neuronas especulares, que pueden ser representados como una ‘descripción’ (la Teoría de Neuronas Especulares) que unifica la comunidad. Puede haber varias descripciones unificadoras.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/def/dul-es"/>
        <rdfs:label xml:lang="es">unifica</rdfs:label>
        <rdfs:label xml:lang="it">unifica</rdfs:label>
        <rdfs:label xml:lang="en">unifies</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/dul-es#usesConcept -->

    <owl:ObjectProperty rdf:about="https://w3id.org/def/dul-es#usesConcept">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/def/dul-es#associatedWith"/>
        <rdfs:domain rdf:resource="https://w3id.org/def/dul-es#Description"/>
        <rdfs:range rdf:resource="https://w3id.org/def/dul-es#Concept"/>
        <rdfs:comment xml:lang="en">A generic relation holding between a Description and a Concept. In order to be used, a Concept must be previously definedIn another Description.</rdfs:comment>
        <rdfs:comment xml:lang="es">Una relación genérica que tiene lugar entre una ‘descripción’ y un ‘concepto’. Para ser usada, un ‘concepto’ debe ser previamente en (‘definido en’) otra descripción.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/def/dul-es"/>
        <rdfs:label xml:lang="it">usa il concetto</rdfs:label>
        <rdfs:label xml:lang="en">uses concept</rdfs:label>
        <rdfs:label xml:lang="es">utiliza concepto</rdfs:label>
    </owl:ObjectProperty>
    


