@prefix : <https://w3id.org/def/otn#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <https://w3id.org/def/otn> .

<https://w3id.org/def/otn> rdf:type owl:Ontology ;
                            owl:imports <https://w3id.org/def/dul-dolce-zero-en-espannol> .

#################################################################
#    Object Properties
#################################################################

###  https://w3id.org/def/otn#coordina
:coordina rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasRole> ;
          owl:inverseOf :tieneCoordinador ;
          rdfs:label "coordina"@es ;
          owl:versionInfo "Cociernta los diferentes elementos del sistema."@es .


###  https://w3id.org/def/otn#esResponsableDe
:esResponsableDe rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasRole> ;
                 rdfs:comment "Tiene a su cargo algo a alguien (inspirado en http://dle.rae.es/?id=WCrxqOp)."@es ;
                 rdfs:label "es responsable de"@es .


###  https://w3id.org/def/otn#esTituloImpartidoPor
:esTituloImpartidoPor rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#actsThrough> ;
                      rdfs:comment "Es la relación inversa de 'imparte título'."@es ;
                      rdfs:label "es título impartido por"@es .


###  https://w3id.org/def/otn#estaBajoLaResponsabilidadDe
:estaBajoLaResponsabilidadDe rdf:type owl:ObjectProperty ;
                             rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isRoleOf> ;
                             rdfs:comment "Es la relación inversa de 'es responsable de'."@es ;
                             rdfs:label "está bajo la responsabilidad de"@es .


###  https://w3id.org/def/otn#imparteTitulo
:imparteTitulo rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#actsFor> ;
               rdfs:comment "Es la realación que se establece entre un centro y un plan de estudios. Un título puede ser impartido en varios centros."@es ;
               rdfs:label "imparte título"@es .


###  https://w3id.org/def/otn#tieneCoordinador
:tieneCoordinador rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isRoleOf> ;
                  rdfs:comment "Es la relación inversa de 'coordina'."@es ;
                  rdfs:label "tiene coordinador"@es .


#################################################################
#    Data properties
#################################################################

###  https://w3id.org/def/otn#tieneNombre
:tieneNombre rdf:type owl:DatatypeProperty ;
             rdfs:comment "Nombre que recibe la entidad."@es ;
             rdfs:label "tiene nombre"@es .


#################################################################
#    Classes
#################################################################

###  http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#InformationObject
<http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#InformationObject> rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                                                            owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#defines> ;
                                                                                            owl:someValuesFrom :ItinerarioFormativo
                                                                                          ] ,
                                                                                          [ rdf:type owl:Restriction ;
                                                                                            owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasPart> ;
                                                                                            owl:someValuesFrom :CreditosECTSDeLaAsignatura
                                                                                          ] .


###  https://w3id.org/def/otn#Asignatura
:Asignatura rdf:type owl:Class ;
            rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/d0.owl#Topic> ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith> ;
                              owl:someValuesFrom :Competencia
                            ] ;
            rdfs:comment "Unidad académica que incluye una serie de contenidos coherentes desde el punto de vista disciplinar de la que es evaluado el estudiante y se le asigna una calificación." ;
            rdfs:label "asignatura"@es .


###  https://w3id.org/def/otn#Aula
:Aula rdf:type owl:Class ;
      rdfs:subClassOf :Sala ;
      rdfs:comment "Sala donde se imparten clases en los centros docentes (http://dle.rae.es/?id=4OCO4gi acepción 1)." ;
      rdfs:label "aula"@es .


###  https://w3id.org/def/otn#AulaInformatica
:AulaInformatica rdf:type owl:Class ;
                 rdfs:subClassOf :Aula ;
                 rdfs:comment "Aula con ordenadores para que puedan trabajar con ellos los alumnos." ;
                 rdfs:label "aula informática"@es .


###  https://w3id.org/def/otn#CalendarioDeImplantacionDelTitulo
:CalendarioDeImplantacionDelTitulo rdf:type owl:Class ;
                                   rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Plan> ;
                                   rdfs:comment "Secuencia temporal en la que se van implantando los diferentes cursos de la titulación (véase el página 62 del Guía de apoyo para la elaboración de la memoria de verificación de títulos oficialaes universitarios (Grado y Máster)  [http://www.aneca.es/content/download/12155/136031/file/verifica_gm_guia_V05.pdf])." ;
                                   rdfs:label "calendario de implantación del título"@es .


###  https://w3id.org/def/otn#CatedraticoDeUniversidad
:CatedraticoDeUniversidad rdf:type owl:Class ;
                          rdfs:subClassOf :Doctor ,
                                          :Profesor ;
                          rdfs:comment "Profesor funcionario doctor con plena capacidad tanto para la docencia como para la investigación que tiene la categoría docente más alta. Para acceder a esta posición es necesario haber obtenido una acreditación nacional cite{leyUniversidades}." ;
                          rdfs:label "catedrático de universidad"@es .


###  https://w3id.org/def/otn#CentroComoDivisionAcademica
:CentroComoDivisionAcademica rdf:type owl:Class ;
                             rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Organization> ,
                                             [ rdf:type owl:Restriction ;
                                               owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasLocation> ;
                                               owl:someValuesFrom :CentroComoLugar
                                             ] ,
                                             [ rdf:type owl:Restriction ;
                                               owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isPartOf> ;
                                               owl:someValuesFrom :Universidad
                                             ] ;
                             rdfs:comment "División académica de una universidad, en la que se agrupan los estudios de disciplinas similares (adaptado de la RAE [http://dle.rae.es/?id=HTxyZDZ]). Un centro está encargado de la organización de las enseñanzas y de los procesos académicos, administrativos y de gestión conducentes a la obtención de títulos universitarios (véase el artículo 8 de la Ley Orgánica 6/2001, de 21 de diciembre, de Universidades [http://www.boe.es/buscar/pdf/2001/BOE-A-2001-24515-consolidado.pdf])." ;
                             rdfs:label "centro como división académica"@es .


###  https://w3id.org/def/otn#CentroComoLugar
:CentroComoLugar rdf:type owl:Class ;
                 rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Place> ;
                 rdfs:comment "Local o conjunto de locales en que funciona un 'centro como división académica' (adaptado de la RAE [http://dle.rae.es/?id=HTxyZDZ])." ;
                 rdfs:label "centro como lugar"@es .


###  https://w3id.org/def/otn#Competencia
:Competencia rdf:type owl:Class ;
             rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/d0.owl#CognitiveEntity> ;
             rdfs:comment "Por competencia se entiende ``el conjunto de conocimientos, habilidades, actitudes que se adquieren o desarrollan mediante experiencias formativas coordinadas, las cuales tienen el propósito de lograr conocimientos funcionales que den respuesta de modo eficiente a una tarea o problema de la vida cotidiana y profesional que requiera un proceso de enseñanza y aprendizaje'.  En el ámbito universitario, las competencias deben ser evaluables y coherentes (véase la página 20 del Guía de apoyo para la elaboración de la memoria de verificación de títulos oficialaes universitarios (Grado y Máster)  [http://www.aneca.es/content/download/12155/136031/file/verifica_gm_guia_V05.pdf])." ;
             rdfs:label "competencia"@es .


###  https://w3id.org/def/otn#CompetenciaBasica
:CompetenciaBasica rdf:type owl:Class ;
                   rdfs:subClassOf :Competencia ;
                   rdfs:comment "la competencias básicas son comunes a la mayoría de los títulos pero están adaptadas al contexto específico de cada uno de ellos. Dentro de este bloque se pueden encontrar competencias personales, competencias interpersonales, etc. (véase la página 20 del Guía de apoyo para la elaboración de la memoria de verificación de títulos oficialaes universitarios (Grado y Máster)  [http://www.aneca.es/content/download/12155/136031/file/verifica_gm_guia_V05.pdf])" ;
                   rdfs:label "competencia básica"@es ,
                              "competencia general"@es .


###  https://w3id.org/def/otn#CompetenciaEspecifica
:CompetenciaEspecifica rdf:type owl:Class ;
                       rdfs:subClassOf :Competencia ;
                       rdfs:comment "las comptencias específicasson propias de un ámbito o título y están orientadas a la consecución de un perfil específico de egresado. En general, acostumbran a tener una proyección longitudinal en el título. (véase la página 20 del Guía de apoyo para la elaboración de la memoria de verificación de títulos oficialaes universitarios (Grado y Máster)  [http://www.aneca.es/content/download/12155/136031/file/verifica_gm_guia_V05.pdf])." ;
                       rdfs:label "competencia específica"@es .


###  https://w3id.org/def/otn#CompetenciaTransversal
:CompetenciaTransversal rdf:type owl:Class ;
                        rdfs:subClassOf :Competencia ;
                        rdfs:comment "las competencias transversales son comunes a todos los estudiantes de una misma Universidad o centro universitario, independientemente del título que cursen. (véase la página 20 del Guía de apoyo para la elaboración de la memoria de verificación de títulos oficialaes universitarios (Grado y Máster)  [http://www.aneca.es/content/download/12155/136031/file/verifica_gm_guia_V05.pdf])." ;
                        rdfs:label "competencia transversal"@es .


###  https://w3id.org/def/otn#CreditoECTS
:CreditoECTS rdf:type owl:Class ;
             rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#UnitOfMeasure> ;
             rdfs:comment "El crédito europeo es la unidad de medida del haber académico que representa la cantidad de trabajo del estudiante para cumplir los objetivos del programa de estudios y que se obtiene por la superación de cada una de las materias que integran los planes de estudios de las diversas enseñanzas conducentes a la obtención de títulos universitarios de carácter oficial y validez en todo el territorio nacional. En esta unidad de medida se integran las enseñanzas teóricas y prácticas, así como otras actividades académicas dirigidas, con inclusión de las horas de estudio y de trabajo que el estudiante debe realizar para alcanzar los objetivos formativos propios de cada una de las materias del correspondiente plan de estudios (véase el artículo 3 del Real Decreto 1125/2003, de 5 de septiembre, por el que se establece el sistema europeo de créditos y el sistema de califica- ciones en las titulaciones universitarias de carácter oficial y validez en todo el territorio nacional.  [https://www.boe.es/boe/dias/2003/09/18/pdfs/A34355-34356.pdf]). El número mínimo de horas, por crédito, será de 25, y el número máximo, de 30 (véase el artículo 4, apartado 5 del Real Decreto 1125/2003, de 5 de septiembre, por el que se establece el sistema europeo de créditos y el sistema de califica- ciones en las titulaciones universitarias de carácter oficial y validez en todo el territorio nacional.  [https://www.boe.es/boe/dias/2003/09/18/pdfs/A34355-34356.pdf])." ;
             rdfs:label "crédito ECTS"@es .


###  https://w3id.org/def/otn#CreditosECTSDeLaAsignatura
:CreditosECTSDeLaAsignatura rdf:type owl:Class ;
                            rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Amount> ,
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isParametrizedBy> ;
                                              owl:someValuesFrom :CreditoECTS
                                            ] ,
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasParameterDataValue> ;
                                              owl:someValuesFrom xsd:int
                                            ] ;
                            rdfs:comment "Esfuerzo estimado, en ECTS, que debe realizar un alumno medio para culminar con éxito la asignatura."@es ;
                            rdfs:label "créditos ECTS de la asignatura"@es .


###  https://w3id.org/def/otn#CreditosECTSDeTitulacion
:CreditosECTSDeTitulacion rdf:type owl:Class ;
                          rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Amount> ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isParametrizedBy> ;
                                            owl:someValuesFrom :CreditoECTS
                                          ] ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasParameterDataValue> ;
                                            owl:someValuesFrom xsd:int
                                          ] ;
                          rdfs:comment "Créditos ECTS necesarios para completar la titulación."@es ;
                          rdfs:label "créditos ECTS de la titulación"@es .


###  https://w3id.org/def/otn#CursoAcademico
:CursoAcademico rdf:type owl:Class ;
                rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#TimeInterval> ;
                rdfs:comment "Intervalo de tiempo anual en el que se desarrolla la actividad académica." ;
                rdfs:label "curso académico"@es .


###  https://w3id.org/def/otn#Departamento
:Departamento rdf:type owl:Class ;
              rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Organization> ;
              rdfs:comment "Unidad de docencia e investigación encargada de coordinar las enseñanzas de uno o varios ámbitos del conocimiento en uno o varios centros, de acuerdo con la programación docente de la universidad, de apoyar las actividades e iniciativas docentes e investigadoras del profesorado, y de ejercer aquellas otras funciones que sean determinadas por los estatutos (véase el artículo 9 de la Ley Orgánica 6/2001, de 21 de diciembre, de Universidades [http://www.boe.es/buscar/pdf/2001/BOE-A-2001-24515-consolidado.pdf])." ;
              rdfs:label "departamento"@es .


###  https://w3id.org/def/otn#DescripcionDeCompetenciaAAdquirir
:DescripcionDeCompetenciaAAdquirir rdf:type owl:Class ;
                                   rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Goal> ,
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#expresses> ;
                                                     owl:someValuesFrom :Competencia
                                                   ] ;
                                   rdfs:comment "Descripción de competencia que se tiene como meta adquirir."@es ;
                                   rdfs:label "descripción de competencia a adquirir"@es .


###  https://w3id.org/def/otn#Doctor
:Doctor rdf:type owl:Class ;
        rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Person> ;
        rdfs:comment "Persona que ha defendido con éxito una tesis doctoral, consistente en un trabajo original de investigación y que, por tanto, posee el título de doctor." ;
        rdfs:label "doctor"@es .


###  https://w3id.org/def/otn#EjecucionDeAsignatura
:EjecucionDeAsignatura rdf:type owl:Class ;
                       rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PlanExecution> ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasPart> ;
                                         owl:someValuesFrom :Enseñanza
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#includesAgent> ;
                                         owl:someValuesFrom :Profesor
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isExpressedBy> ;
                                         owl:someValuesFrom :GuiaDocente
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isSettingFor> ;
                                         owl:someValuesFrom :Asignatura
                                       ] ;
                       rdfs:comment "Siituación en la que la asignatura está en ejecución."@es ;
                       rdfs:label "ejecución de asignatura"@es .


###  https://w3id.org/def/otn#EjecucionDePlanDeEstudios
:EjecucionDePlanDeEstudios rdf:type owl:Class ;
                           rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PlanExecution> ,
                                           [ rdf:type owl:Class ;
                                             owl:unionOf ( [ rdf:type owl:Restriction ;
                                                             owl:onProperty :estaBajoLaResponsabilidadDe ;
                                                             owl:someValuesFrom :CentroComoDivisionAcademica
                                                           ]
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty :estaBajoLaResponsabilidadDe ;
                                                             owl:someValuesFrom :Departamento
                                                           ]
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty :estaBajoLaResponsabilidadDe ;
                                                             owl:someValuesFrom :Instituto
                                                           ]
                                                         )
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasLocation> ;
                                             owl:someValuesFrom :Aula
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasLocation> ;
                                             owl:someValuesFrom :Sala
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasPart> ;
                                             owl:someValuesFrom :EjecucionDeAsignatura
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasParticipant> ;
                                             owl:someValuesFrom :Universidad
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#includesAction> ;
                                             owl:someValuesFrom :Enseñanza
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isExpressedBy> ;
                                             owl:someValuesFrom :MemoriaDeVerificacion
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isSettingFor> ;
                                             owl:someValuesFrom :Idioma
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#satisfies> ;
                                             owl:someValuesFrom :NormativaDePermanencia
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty :esTituloImpartidoPor ;
                                             owl:someValuesFrom :CentroComoDivisionAcademica
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty :tieneCoordinador ;
                                             owl:someValuesFrom :Universidad
                                           ] ;
                           rdfs:comment "Siituación en la que el plan de estudios está en ejecución."@es ;
                           rdfs:label "ejecución de plan de estudios"@es .


###  https://w3id.org/def/otn#EnsenanzaADistancia
:EnsenanzaADistancia rdf:type owl:Class ;
                     rdfs:subClassOf :Enseñanza ;
                     rdfs:comment "Aquélla en que la gran mayoría de las actividades formativas previstas en el Plan de Estudios no requieren la presencia física del estudiante en el centro de impartición del título (véase el página 10 del Guía práctica para la evaluación del seguimiento [http://www.madrimasd.org/uploads/acreditacion/doc/seguimiento2013/Guia_de_Evaluacion_2014.pdf])." ;
                     rdfs:label "enseñanza a distancia"@es .


###  https://w3id.org/def/otn#EnsenanzaPresencial
:EnsenanzaPresencial rdf:type owl:Class ;
                     rdfs:subClassOf :Enseñanza ;
                     rdfs:comment "Aquélla que requiere que el estudiante asista de forma regular y continuada durante todos los cursos a actividades formativas regladas en el centro de impartición del título (véase el página 10 del Guía práctica para la evaluación del seguimiento [http://www.madrimasd.org/uploads/acreditacion/doc/seguimiento2013/Guia_de_Evaluacion_2014.pdf])." ;
                     rdfs:label "enseñanza presencial"@es .


###  https://w3id.org/def/otn#EnsenanzaSemi-presencial
:EnsenanzaSemi-presencial rdf:type owl:Class ;
                          rdfs:subClassOf :Enseñanza ;
                          rdfs:comment "Aquélla en la que la planificación de las actividades formativas previstas en el Plan de Estudios combina la presencia física del estudiante en el centro de impartición del título con un mayor trabajo autónomo del estudiante al propio de la enseñanza presencial (véase el página 10 del Guía práctica para la evaluación del seguimiento [http://www.madrimasd.org/uploads/acreditacion/doc/seguimiento2013/Guia_de_Evaluacion_2014.pdf])." ;
                          rdfs:label "enseñanza semi-presencial"@es .


###  https://w3id.org/def/otn#Enseñanza
:Enseñanza rdf:type owl:Class ;
           rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Action> ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasTimeInterval> ;
                             owl:someValuesFrom :CursoAcademico
                           ] ;
           rdfs:comment "Ejemplo, acción o suceso que sirve de experiencia, enseñando o advirtiendo cómo se debe obrar en casos análogos (http://dle.rae.es/?id=FdHOWng acepción 2)."@es ;
           rdfs:label "enseñanza"@es .


###  https://w3id.org/def/otn#Estudiante
:Estudiante rdf:type owl:Class ;
            rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Role> ;
            rdfs:comment "Toda  persona  que  curse  enseñanzas  oficiales en alguno de los tres ciclos universitarios (Grado, Máster y Doctorado (véase el artículo 8 del Real Decreto 1393/2007, de 29 de octubre, por el que se establece la ordenación de las enseñanzas universitarias oficiales. [https://www.boe.es/boe/dias/2007/10/30/pdfs/A44037-44048.pdf])), enseñanzas de formación continua u otros estudios ofrecidos por las universidades (véase el artículo 1, apartado 3 del Estatuto del Estudiante [https://www.boe.es/boe/dias/2010/12/31/pdfs/BOE-A-2010-20147.pdf])." ;
            rdfs:label "estudiante"@es .


###  https://w3id.org/def/otn#EstudianteATiempoCompleto
:EstudianteATiempoCompleto rdf:type owl:Class ;
                           rdfs:subClassOf :Estudiante ;
